[core]
	autocrlf = input
	symlinks = true
[credential]
	helper = /usr/lib/git-core/git-credential-libsecret
[init]
	defaultBranch = main
[pull]
	rebase = false
[user]
	name = Chris
	email = email@localhost
[includeIf "gitdir:/home/chris/Documents/GitHub/"]
	path = /home/chris/Documents/GitHub/.gitconfig
[includeIf "gitdir:/home/chris/Documents/AUR/"]
	path = /home/chris/Documents/AUR/.gitconfig
[includeIf "gitdir:/home/chris/Documents/eecs/"]
	path = /home/chris/Documents/eecs/.gitconfig
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
