    "cSpell.allowCompoundWords": true,
    "git.showPushSuccessNotification": true,
    "git.enableSmartCommit": true,
    "terminal.external.linuxExec": "gnome-terminal",
    "window.zoomLevel": 0,
    "git.autofetch": true,
    "python.showStartPage": false,
    "extensions.ignoreRecommendations": true,
